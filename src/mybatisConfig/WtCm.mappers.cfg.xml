<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
  <!-- 类别名设置 -->
  <typeAliases>
    <!-- 电台相关 -->
    <typeAlias alias="broadcast" type="com.woting.content.broadcast.persistence.pojo.BroadcastPo"/>
    <typeAlias alias="bc_liveflow" type="com.woting.content.broadcast.persistence.pojo.LiveFlowPo"/>
    <typeAlias alias="bc_frequnce" type="com.woting.content.broadcast.persistence.pojo.FrequncePo"/>
    <!-- 公共关系
    <typeAlias alias="ref_catalog" type="com.woting.content.pubref.persistence.pojo.ResCataRefPo"/> -->
    <!-- 版本管理 -->
    <typeAlias alias="PVersion"     type="com.woting.version.core.model.Version"/>
    <typeAlias alias="PVerConfig"   type="com.woting.version.core.model.VersionConfig"/>
  </typeAliases>
  <mappers>
    <!-- 电台相关 -->
    <mapper resource="com/woting/content/broadcast/persistence/sql/Broadcast.mapper.xml"/>
    <mapper resource="com/woting/content/broadcast/persistence/sql/Frequnce.mapper.xml"/>
    <mapper resource="com/woting/content/broadcast/persistence/sql/LiveFlow.mapper.xml"/>
    <!-- 公共关系
    <mapper resource="com/woting/content/pubref/persistence/sql/ResCataRef.mapper.xml"/> -->
    <!-- 版本管理 -->
    <mapper resource="com/woting/version/core/persis/sql/Version.mapper.xml"/>
  </mappers>
</configuration>